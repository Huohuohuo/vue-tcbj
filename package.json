{
  "name": "vue-tcbj",
  "description": "A plugin for Vue.js",
  "version": "1.0.9",
  "author": "Daniel",
  "license": "MIT",
  "private": false,
  "main": "dist/vue-tcbj.js",
  "scripts": {
    "j2m": "jsdoc2md src/lib/index.js > docs/api.md",
    "docs": "docsify serve docs",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.config.js --hot",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --progress --hide-modules",
    "buildHtml": "cross-env NODE_ENV=production webpack --config webpack.buildHtml.config.js --progress --hide-modules"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.11",
    "we-vue": "^2.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jsdoc-to-markdown": "^4.0.1",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Huohuohuo/vue-tcbj"
  }
}
